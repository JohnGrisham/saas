{
  "name": "templates",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./play-template.html": "./play-tailwind-main/dist/index.html"
  },
  "license": "MIT",
  "scripts": {
    "build:play": "cd play-tailwind-main && yarn build",
    "dev:play": "cd play-tailwind-main && yarn dev",
    "build": "yarn build:play && tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/play.tsx --format esm,cjs --dts",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@parcel/transformer-typescript-tsc": "^2.8.3",
    "concurrently": "^7.2.2",
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "parcel": "^2.8.3",
    "tailwind-config": "*",
    "tsconfig": "*",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "core": "*",
    "dangerously-set-html-content": "^1.0.13",
    "yup": "^1.0.2"
  }
}
