{
  "name": "saas",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "gql:dev": "npx grafbase dev",
    "codegen": "turbo run codegen",
    "dev": "turbo run dev --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint"
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "latest",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.14.0"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.15"
}
